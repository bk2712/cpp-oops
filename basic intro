#include <iostream>

using namespace std;

class Student{
    public:
    string name= "bhavesh";
    int age= 21;
    int roll_no= 79;
};

int main()
{
    Student st;
    cout<<"student name is: "<<st.name<<endl;
    cout<<"student age is: "<<st.age<<endl;
    cout<<"student roll_no is: "<<st.roll_no<<endl;

    return 0;
}
